{
  "manifest_version": 2,
  "name": "Name Correcter",
  "version": "1.1",
  "description": "Correctly names famous people who have been incorrectly identified",
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "icons": { "128": "icon_128.png", "48": "icon_48.png", "32": "icon_32.png"},
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}