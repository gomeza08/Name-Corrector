{
  "manifest_version": 2,
  "name": "Name Correcter",
  "version": "1.0",
  "description": "Correctly names famous people who have been incorrectly identified",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}